      "title": "Voice Status",
      "type": "stat"
    },
    {
      "datasource": { "type": "prometheus", "uid": "prometheus" },
      "fieldConfig": {
        "defaults": {
          "color": { "mode": "thresholds" },
          "mappings": [
            { "options": { "0": { "color": "red", "text": "DOWN" }, "1": { "color": "green", "text": "UP" } }, "type": "value" }
          ],
          "thresholds": { "mode": "absolute", "steps": [{ "color": "red", "value": null }, { "color": "green", "value": 1 }] }
        }
      },
      "gridPos": { "h": 4, "w": 3, "x": 6, "y": 1 },
      "id": 4,
      "targets": [{ "expr": "mongodb_up", "refId": "A" }],
      "title": "MongoDB Status",
      "type": "stat"
    },
    {
      "datasource": { "type": "prometheus", "uid": "prometheus" },
      "fieldConfig": {
        "defaults": {
          "color": { "mode": "thresholds" },
          "mappings": [
            { "options": { "0": { "color": "red", "text": "DOWN" }, "1": { "color": "green", "text": "UP" } }, "type": "value" }
          ],
          "thresholds": { "mode": "absolute", "steps": [{ "color": "red", "value": null }, { "color": "green", "value": 1 }] }
        }
      },
      "gridPos": { "h": 4, "w": 3, "x": 9, "y": 1 },
      "id": 5,
      "targets": [{ "expr": "redis_up", "refId": "A" }],
      "title": "Redis Status",
      "type": "stat"
    },
    {
      "datasource": { "type": "prometheus", "uid": "prometheus" },
      "fieldConfig": {
        "defaults": { "color": { "mode": "palette-classic" }, "unit": "percent" }
      },
      "gridPos": { "h": 4, "w": 6, "x": 12, "y": 1 },
      "id": 6,
      "targets": [{ "expr": "100 - (avg(irate(node_cpu_seconds_total{mode=\"idle\"}[5m])) * 100)", "legendFormat": "CPU Usage", "refId": "A" }],
      "title": "CPU Usage",
      "type": "gauge"
    },
    {
      "datasource": { "type": "prometheus", "uid": "prometheus" },
      "fieldConfig": {
        "defaults": { "color": { "mode": "palette-classic" }, "unit": "percent" }
      },
      "gridPos": { "h": 4, "w": 6, "x": 18, "y": 1 },
      "id": 7,
      "targets": [{ "expr": "(node_memory_MemTotal_bytes - node_memory_MemAvailable_bytes) / node_memory_MemTotal_bytes * 100", "legendFormat": "Memory Usage", "refId": "A" }],
      "title": "Memory Usage",
      "type": "gauge"
    },
    {
      "collapsed": false,
      "gridPos": { "h": 1, "w": 24, "x": 0, "y": 5 },
      "id": 10,
      "panels": [],
      "title": "Request Metrics",
      "type": "row"
    },
    {
      "datasource": { "type": "prometheus", "uid": "prometheus" },
      "fieldConfig": { "defaults": { "color": { "mode": "palette-classic" }, "unit": "reqps" } },
      "gridPos": { "h": 8, "w": 12, "x": 0, "y": 6 },
      "id": 11,
      "targets": [
        { "expr": "sum(rate(http_requests_total[5m]))", "legendFormat": "Total", "refId": "A" },
        { "expr": "sum(rate(http_requests_total{status=~\"2..\"}[5m]))", "legendFormat": "2xx", "refId": "B" },
        { "expr": "sum(rate(http_requests_total{status=~\"4..\"}[5m]))", "legendFormat": "4xx", "refId": "C" },
        { "expr": "sum(rate(http_requests_total{status=~\"5..\"}[5m]))", "legendFormat": "5xx", "refId": "D" }
      ],
      "title": "Request Rate",
      "type": "timeseries"
    },
    {
      "datasource": { "type": "prometheus", "uid": "prometheus" },
      "fieldConfig": { "defaults": { "color": { "mode": "palette-classic" }, "unit": "s" } },
      "gridPos": { "h": 8, "w": 12, "x": 12, "y": 6 },
      "id": 12,
      "targets": [
        { "expr": "histogram_quantile(0.50, sum(rate(http_request_duration_seconds_bucket[5m])) by (le))", "legendFormat": "p50", "refId": "A" },
        { "expr": "histogram_quantile(0.90, sum(rate(http_request_duration_seconds_bucket[5m])) by (le))", "legendFormat": "p90", "refId": "B" },
        { "expr": "histogram_quantile(0.95, sum(rate(http_request_duration_seconds_bucket[5m])) by (le))", "legendFormat": "p95", "refId": "C" },
        { "expr": "histogram_quantile(0.99, sum(rate(http_request_duration_seconds_bucket[5m])) by (le))", "legendFormat": "p99", "refId": "D" }
      ],
      "title": "Response Time Percentiles",
      "type": "timeseries"
    },
    {
      "collapsed": false,
      "gridPos": { "h": 1, "w": 24, "x": 0, "y": 14 },
      "id": 20,
      "panels": [],
      "title": "Database Metrics",
      "type": "row"
    },
    {
      "datasource": { "type": "prometheus", "uid": "prometheus" },
      "fieldConfig": { "defaults": { "unit": "short" } },
      "gridPos": { "h": 6, "w": 8, "x": 0, "y": 15 },
      "id": 21,
      "targets": [{ "expr": "mongodb_connections_current", "legendFormat": "Current", "refId": "A" }, { "expr": "mongodb_connections_available", "legendFormat": "Available", "refId": "B" }],
      "title": "MongoDB Connections",
      "type": "timeseries"
    },
    {
      "datasource": { "type": "prometheus", "uid": "prometheus" },
      "fieldConfig": { "defaults": { "unit": "bytes" } },
      "gridPos": { "h": 6, "w": 8, "x": 8, "y": 15 },
      "id": 22,
      "targets": [{ "expr": "redis_memory_used_bytes", "legendFormat": "Used", "refId": "A" }, { "expr": "redis_memory_max_bytes", "legendFormat": "Max", "refId": "B" }],
      "title": "Redis Memory",
      "type": "timeseries"
    },
    {
      "datasource": { "type": "prometheus", "uid": "prometheus" },
      "fieldConfig": { "defaults": { "unit": "short" } },
      "gridPos": { "h": 6, "w": 8, "x": 16, "y": 15 },
      "id": 23,
      "targets": [{ "expr": "redis_connected_clients", "legendFormat": "Clients", "refId": "A" }],
      "title": "Redis Clients",
      "type": "timeseries"
    },
    {
      "collapsed": false,
      "gridPos": { "h": 1, "w": 24, "x": 0, "y": 21 },
      "id": 30,
      "panels": [],
      "title": "Voice Server Metrics",
      "type": "row"
    },
    {
      "datasource": { "type": "prometheus", "uid": "prometheus" },
      "fieldConfig": { "defaults": { "unit": "short" } },
      "gridPos": { "h": 6, "w": 8, "x": 0, "y": 22 },
      "id": 31,
      "targets": [{ "expr": "voice_active_sessions", "legendFormat": "Active Sessions", "refId": "A" }],
      "title": "Voice Active Sessions",
      "type": "timeseries"
    },
    {
      "datasource": { "type": "prometheus", "uid": "prometheus" },
      "fieldConfig": { "defaults": { "unit": "s" } },
      "gridPos": { "h": 6, "w": 8, "x": 8, "y": 22 },
      "id": 32,
      "targets": [{ "expr": "histogram_quantile(0.95, sum(rate(voice_session_latency_seconds_bucket[5m])) by (le))", "legendFormat": "p95 Latency", "refId": "A" }],
      "title": "Voice Latency (p95)",
      "type": "timeseries"
    },
    {
      "datasource": { "type": "prometheus", "uid": "prometheus" },
      "fieldConfig": { "defaults": { "unit": "bytes" } },
      "gridPos": { "h": 6, "w": 8, "x": 16, "y": 22 },
      "id": 33,
      "targets": [{ "expr": "rate(voice_audio_bytes_received_total[5m])", "legendFormat": "Received", "refId": "A" }, { "expr": "rate(voice_audio_bytes_sent_total[5m])", "legendFormat": "Sent", "refId": "B" }],
      "title": "Voice Audio Throughput",
      "type": "timeseries"
    }
  ],
